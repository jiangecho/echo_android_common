<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MinSizeImageView">
        <attr name="minWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="microSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WPTextView">
        <attr name="fontVariation" format="enum">
            <enum name="normal" value="0" />
            <enum name="light" value="1" />
        </attr>
        <attr name="fixWidowWords" format="boolean"/>
    </declare-styleable>

</resources>